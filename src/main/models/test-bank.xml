<?xml version="1.0" encoding="UTF-8"?>
<dataset name="test-bank">
	<storage type="Elasticsearch">
		<indices>
			<index>test-bank</index>
		</indices>
	</storage>

	<attributes>
		<attribute type="NONE" name="id"></attribute>
		<attribute type="NUMERIC" name="age"></attribute>
		<attribute type="NOMINAL" name="sex">
			<class name="MALE" />
			<class name="FENAMLE" />
		</attribute>
		<attribute type="NONE" name="region"></attribute>
		<attribute type="NUMERIC" name="income"></attribute>
		<attribute type="NOMINAL" name="save_act">
			<class name="YES" />
			<class name="NO" />
		</attribute>
		<attribute type="NOMINAL" name="pep">
			<class name="YES" />
			<class name="NO" />
		</attribute>
		<attribute type="NOMINAL" name="married">
			<class name="YES" />
			<class name="NO" />
		</attribute>
		<attribute type="NON-NUMERIC" name="mortgage">
			<class name="YES" />
			<class name="NO" />
		</attribute>
		<attribute type="NUMERIC" name="children"></attribute>
		<attribute type="NON-NUMERIC" name="car">
			<class name="YES" />
			<class name="NO" />
		</attribute>
		<attribute type="NON-NUMERIC" name="current_act">
			<class name="YES" />
			<class name="NO" />
		</attribute>
	</attributes>

	<observation>1000</observation>

	<preprocessing>
		<missingValue type="delete"></missingValue>
		<normalization type="minmax"></normalization>
	</preprocessing>

</dataset>